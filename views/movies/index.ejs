	<div class="row1 my-5">
		<h1 class="text-4xl ml-5 pb-5 font-extrabold font-blue text-white">Horrors</h1>
		<ul>
			<div class="flex">
			<% for (let movie of movies) { if(movie.genre == "horror"){%>
				<div class="px-8">
					<a href="/movies/<%= movie._id %>"><img class="w-48 rounded-md" src="<%= movie.image %>" alt=""></a>
				</div>
			<% } } %>
			</div>
		</ul>
	</div>
	<div class="row2 my-5">
		<h1 class="text-4xl ml-5 pb-5 font-extrabold font-blue text-white">TV Comedies</h1>
		<ul>
			<div class="flex">
			<% for (let movie of movies) { if(movie.genre == "Comedy"){%>
				<div class="px-8">
					<a href="/movies/<%= movie._id %>"><img class="w-48 rounded-md" src="<%= movie.image %>" alt=""></a>
				</div>
			<% } } %>
			</div>
		</ul>
	</div>
	<div class="row3 my-5">
		<h1 class="text-4xl ml-5 pb-5 font-extrabold font-blue text-white">Thrillers</h1>
		<ul>
			<div class="flex">
			<% for (let movie of movies) { if(movie.genre == "Thriller"){%>
				<div class="px-8">
					<a href="/movies/<%= movie._id %>"><img class="w-48 rounded-md" src="<%= movie.image %>" alt=""></a>
				</div>
			<% } } %>
			</div>
		</ul>
	</div>
</main>
